{
  "name": "tradeup-payment-server",
  "version": "1.0.0",
  "description": "Payment server for TradeUp mobile app using Stripe",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "echo \"No tests yet\" && exit 0",
    "setup": "npm install && echo \"Setup completed! Run 'npm start' to start the server\"",
    "android-clean": "cd android && ./gradlew clean",
    "android-build": "cd android && ./gradlew assembleDebug"
  },
  "dependencies": {
    "express": "^4.18.2",
    "stripe": "^14.13.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  },
  "keywords": ["stripe", "payment", "android", "nodejs", "express"],
  "author": "TradeUp Team",
  "license": "MIT"
}
